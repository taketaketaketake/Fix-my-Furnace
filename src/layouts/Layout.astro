---
import Header from '../components/Header.astro';
import '../styles/global.css';
export interface Props {
	title: string;
}
const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{title}</title>
</head>
<body>
    <Header />
    <main>
        <slot />
    </main>
</body>
</html>